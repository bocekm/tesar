{
  "api_key": "$apikey",
  "test": {
    "fmf": {
      "url": "$fmf-git-url",
      "ref": "$fmf-branch-reference",
      "path": "$git-path",
      "name": "$plan-name"
    }
  },
  "environments": [
    {
      "arch": "$arch",
      "os": {
        "compose": "$compose"
      },
      "artifacts": [
        {
          "id": "$artifact-id",
          "type": "$artifact-type",
          "packages": [
            "$packages"
          ]
        }
      ],
      "tmt": {
        "context": {
          "distro": "$tmt-distro",
          "arch": "$tmt-arch"
        }
      }
    }
  ]
}